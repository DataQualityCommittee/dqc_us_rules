AccountingForCertainLoansAndDebtSecuritiesAcquiredInTransferDisclosureTextBlock);

exists({@concept in ($ASU201613_TransitionELements - $ExceptionsToASU201613) where $fact.period.end > date('2023-09-30')}#TransFact)

